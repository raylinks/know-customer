{
  "name": "scaffold",
  "version": "1.0.0",
  "description": "Boilerplate code starter for  backend services",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "ts-node server/swagger.ts && nodemon server/index.ts | pino-pretty",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "test": "jest",
    "prepare": "husky",
    "swagger": "ts-node server/swagger.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ./server/data-source.ts",
    "generate": "npm run typeorm migration:generate ./server/api/database/migrations/kyc-migrations",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d  ./server/data-source.ts",
    "schema:sync": "npm run typeorm schema:sync",
    "schema:drop": "npm run typeorm schema:drop"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.485.0",
    "@aws-sdk/client-sqs": "^3.490.0",
    "@google-cloud/secret-manager": "^5.0.1",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.2",
    "@opentelemetry/sdk-metrics": "^1.19.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@types/multer": "^1.4.11",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "buffer-to-stream": "^1.0.0",
    "cloudinary": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "opossum": "^8.1.3",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.2.1",
    "sqs-consumer": "^8.1.3",
    "swagger-autogen": "^2.23.7",
    "swagger-express-middleware": "^4.0.2",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0",
    "unleash-client": "^5.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/buffer-to-stream": "^1.0.3",
    "@types/chai": "^4.3.11",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "@types/opossum": "^8.1.6",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^6.0.2",
    "@types/swagger-express-middleware": "^1.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "shelljs": "^0.8.5",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
